{
  "hosting": {
    "public": "src",
    "ignore": [
      "devcon.html",
      "devbiz.html",
      "images/Archived",
      "images/Blog",
      "**/.*"
    ],
    "rewrites": [
      {
        "source": "/business",
        "destination": "/business.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}